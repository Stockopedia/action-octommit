{
  "name": "action-octommit",
  "private": true,
  "version": "3.0.0",
  "description": "Octommit github action",
  "main": "lib/main.js",
  "scripts": {
    "build": "rm -rf lib/* && tsc -p ./tsconfig.build.json",
    "format": "prettier --write **/*.ts",
    "format-check": "prettier --check **/*.ts",
    "lint": "eslint src/**/*.ts",
    "package": "ncc build --source-map --license licenses.txt",
    "test": "jest",
    "all": "npm run build && npm run format && npm run package"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stockopedia/action-octommit.git"
  },
  "keywords": [
    "actions",
    "node",
    "setup",
    "gitops"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/io": "^1.1.3",
    "@stockopedia/octommit": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.42.0",
    "jest": "^29.5.0",
    "jest-circus": "^29.5.0",
    "js-yaml": "^4.1.0",
    "prettier": "2.8.8",
    "ts-jest": "^29.1.0",
    "typescript": "^5.1.3"
  }
}
